<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.georgewpurnell.studyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewQuestions" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".Quiz" />
        <activity android:name=".FlashCard" />
        <activity android:name=".LearnMore" />
        <activity android:name=".MultipleChoice" />
        <activity android:name=".TrueFalse" />
        <activity android:name=".FillInBlank"></activity>
    </application>

</manifest>